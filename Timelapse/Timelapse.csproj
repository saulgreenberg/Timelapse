<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <OutputType>WinExe</OutputType>
    <RootNamespace>Timelapse</RootNamespace>
    <AssemblyName>Timelapse</AssemblyName>
    <ApplicationIcon>Icons\Timelapse.ico</ApplicationIcon>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <NoWarn>CA1416;CS8981;SYSLIB1045;IDE0130</NoWarn>

    <!-- Platform target for all builds -->
    <PlatformTarget>x64</PlatformTarget>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>

    <!-- Assembly version - increment for each release -->
    <AssemblyVersion>2.4.0.0</AssemblyVersion>
    <FileVersion>2.4.0.0</FileVersion>

    <!-- NOTE: Additional assembly attributes (ThemeInfo, InternalsVisibleTo) are defined in Properties/AssemblyAttributes.cs -->

    <!-- Assembly metadata -->
    <Company>Greenberg Consulting Inc. &amp; University of Calgary</Company>
    <Product>Timelapse</Product>
    <Description>A tool to simplify analysis of images and videos captured from remote cameras.</Description>
    <Copyright>Copyright © 2011 +</Copyright>
    <NeutralLanguage>en-US</NeutralLanguage>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;DEBUG</DefineConstants>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsPublishable>False</IsPublishable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants></DefineConstants>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsPublishable>True</IsPublishable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <DefineConstants>TRACE;DEBUG</DefineConstants>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsPublishable>False</IsPublishable>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <DefineConstants></DefineConstants>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsPublishable>True</IsPublishable>
  </PropertyGroup>

  <!-- Exclude design-time assemblies from Release builds -->
  <ItemGroup Condition="'$(Configuration)'=='Release'">
    <RuntimeHostConfigurationOption Include="Microsoft.NETCore.App.ExcludeFromPublish.System.Windows.Forms.Design" Value="true" />
    <RuntimeHostConfigurationOption Include="Microsoft.NETCore.App.ExcludeFromPublish.System.Windows.Forms.Design.Editors" Value="true" />
    <RuntimeHostConfigurationOption Include="Microsoft.NETCore.App.ExcludeFromPublish.System.Drawing.Design" Value="true" />
    <RuntimeHostConfigurationOption Include="Microsoft.NETCore.App.ExcludeFromPublish.System.Design" Value="true" />
  </ItemGroup>

  <!-- Remove design-time and unused DLLs after build -->
  <Target Name="RemoveUnusedDlls" AfterTargets="Build" Condition="'$(Configuration)'=='Release'">
    <ItemGroup>
      <!-- Design-time DLLs -->
      <UnusedDlls Include="$(OutputPath)**\System.Windows.Forms.Design.dll" />
      <UnusedDlls Include="$(OutputPath)**\xSystem.Windows.Forms.Design.dll" />
      <UnusedDlls Include="$(OutputPath)**\System.Windows.Forms.Design.Editors.dll" />
      <UnusedDlls Include="$(OutputPath)**\xSystem.Windows.Forms.Design.Editors.dll" />
      <UnusedDlls Include="$(OutputPath)**\System.Drawing.Design.dll" />
      <UnusedDlls Include="$(OutputPath)**\System.Design.dll" />
      <!-- Microsoft.VisualBasic DLLs (no longer needed after CsvHelper migration) -->
      <UnusedDlls Include="$(OutputPath)**\Microsoft.VisualBasic.Core.dll" />
      <UnusedDlls Include="$(OutputPath)**\Microsoft.VisualBasic.Forms.dll" />
      <UnusedDlls Include="$(OutputPath)**\Microsoft.VisualBasic.dll" />
      <!-- Legacy WPF theme DLLs (only Aero2 is needed for Windows 10/11) -->
      <UnusedDlls Include="$(OutputPath)**\PresentationFramework.Luna.dll" />
      <UnusedDlls Include="$(OutputPath)**\PresentationFramework.Aero.dll" />
      <UnusedDlls Include="$(OutputPath)**\PresentationFramework.Royale.dll" />
      <UnusedDlls Include="$(OutputPath)**\PresentationFramework.Classic.dll" />
      <UnusedDlls Include="$(OutputPath)**\PresentationFramework.AeroLite.dll" />
      <!-- Debug/Diagnostic DLLs (not needed for normal runtime execution)
           CAUTION: Removing these means:
           - Users cannot attach debuggers to diagnose crashes
           - Crash dumps will have limited debugging information
           - Production issue diagnosis will be harder
           RECOMMENDATION: Keep good error logging/telemetry in your application -->
      <UnusedDlls Include="$(OutputPath)**\mscordaccore.dll" />
      <UnusedDlls Include="$(OutputPath)**\mscordaccore_amd64_amd64*.dll" />
      <UnusedDlls Include="$(OutputPath)**\mscordbi.dll" />
      <UnusedDlls Include="$(OutputPath)**\Microsoft.DiaSymReader.Native.amd64.dll" />
      <!-- Additional unused DLLs (included by .NET runtime but not referenced in code) -->
      <UnusedDlls Include="$(OutputPath)**\System.DirectoryServices.dll" />
    </ItemGroup>
    <Delete Files="@(UnusedDlls)" />
    <Message Text="Removed design-time, unused, legacy theme, and debug DLLs from Release build" Importance="high" />
  </Target>


